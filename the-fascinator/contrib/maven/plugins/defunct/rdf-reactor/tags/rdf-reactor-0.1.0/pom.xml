				<artifactId>maven-plugin-plugin</artifactId>
				<version>2.5.1</version>
			</plugin>
		</plugins>
	</reporting>
	<repositories>
		<repository>
			<id>adfi-usq-releases</id>
			<url>http://fascinator.usq.edu.au/nexus/content/groups/public</url>
